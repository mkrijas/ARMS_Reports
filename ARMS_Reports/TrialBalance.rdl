  <Code>Public Shared Function BuildIntegerListInserts(ByVal variableName As String, ByVal paramValues As Object()) As String
  Dim insertStatements As New System.Text.StringBuilder()
  For Each paramValue As Object In paramValues
    insertStatements.AppendLine(String.Format("INSERT {0} VALUES ({1})", variableName, paramValue))
  Next
 
  Return insertStatements.ToString()
End Function</Code>
